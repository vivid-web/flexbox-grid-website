@import url('https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700');
@font-face {
  font-family: 'Fira Code';
  src: url('~@/assets/fonts/fira-code/eot/fira-code-light.eot');
  src: url('~@/assets/fonts/fira-code/eot/fira-code-light.eot') format('embedded-opentype'),
    url('~@/assets/fonts/fira-code/woff2/fira-code-light.woff2') format('woff2'),
    url('~@/assets/fonts/fira-code/woff/fira-code-light.woff') format('woff'),
    url('~@/assets/fonts/fira-code/ttf/fira-code-light.ttf') format('truetype');
  font-weight: 300;
  font-style: normal;
}

