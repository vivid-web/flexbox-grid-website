@import url("https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700");
@font-face{
  font-family: 'Fira Code';
  src: url('../fonts/fira-code/light/fira-code-light.eot');
  src: url('../fonts/fira-code/light/fira-code-light.eot') format('embedded-opentype'),
          url('../fonts/fira-code/light/fira-code-light.woff2') format('woff2'),
          url('../fonts/fira-code/light/fira-code-light.woff') format('woff'),
          url('../fonts/fira-code/light/fira-code-light.ttf') format('truetype');
  font-weight: 300;
  font-style: normal;
}

